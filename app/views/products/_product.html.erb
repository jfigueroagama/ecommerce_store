<div class="card">
  <div class="card-body">
    <div class="card-title">
      <%= "#{product.name}" %>
    </div>
    <div class="card-text">
      <p><small> SGD </small> <%= "#{product.price}" %></p>
      <div class="card-footer">
        <%= link_to 'View Product', product_path(product) %>
      </div>
    </div>
  </div>
</div>
